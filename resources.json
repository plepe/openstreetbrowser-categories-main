            "relation[man_made~\"^(mineshaft)$\"];",
            ")"
        ]
    },
    "feature": {
        "pre": [
            "{% if tags.landuse in [ 'quarry', 'salt_pond' ] %}",
            "  {% set key = 'landuse' %}",
            "  {% set value = tags.landuse %}",
            "{% else %}",
            "  {% set key = 'man_made' %}",
            "  {% set value = tags.man_made %}",
            "{% endif %}"
        ],
        "description": "{{ tagTrans(key, value) }}"
    }
