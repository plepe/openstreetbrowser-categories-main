            "priority": 2
        },
        "amenity=bicycle_parking": {
            "sign": "ðŸ…¿",
            "priority": {
                "capacity": [
                    [
                        0,
                        3
                    ],
                    [
                        20,
                        2
                    ],
                    [
                        100,
                        0
                    ]
                ],
                "default": 3
            }
        },
        "amenity=bicycle_rental": {
